package com.jacharat;

public class CountDigits {
    public static void main(String[] args) {
        String input = "Java1234";
        int digits = 0;
        for (int i = 0; i < input.length(); i++) {
            char ch = input.charAt(i);
            if (ch >= '0' && ch <= '9') digits++;
        }
        System.out.println("Digits = " + digits);
    }
}
